@model List<P4___Distributed_Fault_Tolerance.Models.GradeModel>

<h2>Your Grades</h2>

<table class="table">
    <thead>
		<tr>
			<th>Course</th>
			<th>Grade</th>
		</tr>
    </thead>
	<tbody>
        @foreach (var grade in Model)
        {
            <tr>
                <td>@grade.CourseName</td>
                <td>@grade.GradeValue</td>
            </tr>
        }
	</tbody>
</table>